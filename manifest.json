{
	"name": "Extension for moders",
	"version": "1.3.0",
	"description": "Расширение для модераторов dota2.ru",
	"manifest_version": 2,
	"icons": {
		"16": "img/16x16.png",
		"32": "img/32x32.png",
		"48": "img/48x48.png",
		"128": "img/128x128.png"
	},
	"content_scripts": [{
		"css": [ "css/style.css" ],
		"js": [ "background.js" ],
		"matches": [ "*://dota2.ru/forum*" ],
		"run_at": "document_start",
		"persistent": false
	}],
	"browser_action": {
		"default_popup": "html/popup.html"
	},
	"web_accessible_resources": [ "*" ]
}
